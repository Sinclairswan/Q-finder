
<html>

<body >
  <h1 class="page-header">Qfinder</h1>
  <div class="background-container">
      <% if current_user %>
      <%= render "profile" %>
      <%end %>

       <div class="background">

        <div class="wrap">
         <div class="search">
            <input type="text" class="searchTerm" placeholder="What city are you looking for?">
            <button type="submit" class="searchButton">
              <i class="fa fa-search"></i>
           </button>
         </div>
      </div>

     </div>
  </div>
</body>
</html>






